{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "pages",
  "info": {
    "name": "Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "content": {
      "type": "dynamiczone",
      "components": [
        "special.page-section-picker",
        "atoms.rich-text",
        "atoms.heading",
        "atoms.image",
        "atoms.html",
        "organisms.blog-list",
        "organisms.signup-starter",
        "organisms.learn-more-button",
        "organisms.sign-up-form",
        "special.special-video-bl-ock"
      ]
    },
    "title": {
      "type": "string"
    },
    "share_image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "sections": {
      "collection": "page-section",
      "via": "pages"
    },
    "tags": {
      "via": "pages",
      "collection": "tags",
      "dominant": true
    }
  }
}
