{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "blogs",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "text": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "tags": {
      "via": "blogs",
      "collection": "tags",
      "dominant": true
    }
  }
}
